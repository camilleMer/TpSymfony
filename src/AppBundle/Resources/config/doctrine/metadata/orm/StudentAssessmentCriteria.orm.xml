<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="StudentAssessmentCriteria" table="student_assessment_criteria">
    <indexes>
      <index name="fk_student_assessment_criteria_criteria1_idx" columns="criteria_id"/>
      <index name="fk_student_assessment_criteria_student1_idx" columns="student_id"/>
      <index name="fk_student_assessment_criteria_assessment1_idx" columns="assessment_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="mark" type="integer" column="mark" nullable="true"/>
    <many-to-one field="assessment" target-entity="Assessment">
      <join-columns>
        <join-column name="assessment_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="student" target-entity="Student">
      <join-columns>
        <join-column name="student_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="criteria" target-entity="Criteria">
      <join-columns>
        <join-column name="criteria_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
